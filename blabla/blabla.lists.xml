<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Iker Azpeitia</author>
    <documentationURL>http://rifers.org/wiki/display/BLA/REST%20API.html</documentationURL>
    <sampleQuery>SELECT * FROM {table} WHERE login='oscaronekin' and password='12osin34';</sampleQuery>     
  </meta>
  <bindings>
    <select itemPath="" produces="XML">
      <urls>
      <url>http://blablalist.com/fetchlists?submission=credentials</url>       
      </urls>
      <inputs>
         <key id="login" type="xs:string" paramType="query" required="true"/>
         <key id="password" type="xs:string" paramType="query" required="true"/>
      </inputs>
    </select>
  </bindings>
</table>

