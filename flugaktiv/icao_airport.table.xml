<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
<meta>
<sampleQuery>			select * from {table} where icao="EDDF"		</sampleQuery>
<description>Airport Information lookup based on IATA Code, ICAO Code or fulltext search</description>
<documentationURL>http://flugaktiv.de/</documentationURL>
</meta>
<bindings>
<select itemPath="flugaktiv.airport" produces="XML">
<urls>
<url>http://flugaktiv.de/db/icao_airport.php</url>
</urls>
<inputs>
<key id="iata" type="xs:string" required="false" paramType="query"/>
<key id="query" type="xs:string" required="false" paramType="query"/>
<key id="icao" type="xs:string" required="false" paramType="query"/>
</inputs>
</select>
</bindings>
</table>
