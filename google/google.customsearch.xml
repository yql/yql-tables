<?xml version="1.0" encoding="UTF-8" ?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
    <meta>
        <author>Sebastian Spier (http://twitter.com/#!/sebastianspier/)</author>
        <documentationURL>
            The new custom search API from Google.
            Important: This version of the JSON/Atom Custom Search API is in Labs, and its features might change unexpectedly until it graduates. 
            http://code.google.com/intl/de/apis/customsearch/v1/getting_started.html
        </documentationURL>
        <sampleQuery>select * from {table} where q = "pizza"</sampleQuery>
    </meta>
    <bindings>
        <select itemPath="" produces="JSON">
            <urls>
                <url>https://www.googleapis.com/customsearch/v1</url>
            </urls>
            <inputs>
                <key id='q' type='xs:string' paramType='query' required="true"/>
                <key id='cx' type='xs:string' paramType='query' required="true" default="017576662512468239146:omuauf_lfve"/>
                <key id='cref' type='xs:string' paramType='query'/>
                <key id='safe' type='xs:string' paramType='query'/>
                <key id='key' as="api-key" type='xs:string' paramType='query' required="true"/>
            </inputs>
        </select>
    </bindings>
</table>
