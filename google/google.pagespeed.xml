<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
	<meta>
		<author>Preetam Jinka</author>
		<description>Google Pagespeed API</description>
		<documentationURL>https://code.google.com/apis/pagespeedonline/v1/getting_started.html</documentationURL>
	</meta>
	<bindings>
		<select itemPath="" produces="JSON" >
			<urls>
				<url env="all">https://www.googleapis.com/pagespeedonline/v1/runPagespeed?url={url}</url>
			</urls>
			<inputs>
				<key id="url" type="xs:string" paramType="path" required="true" />
				<key id="key" type="xs:string" paramType="query" required="true" />
			</inputs>
		</select>
	</bindings>
</table>
