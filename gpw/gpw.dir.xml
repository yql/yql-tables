<?xml version="1.0" encoding="UTF-8"?>

<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Kuba Ku≈∫ma</author>
    <sampleQuery>SELECT * FROM gpw.dir;</sampleQuery>
    <documentationURL></documentationURL>
  </meta>
  <bindings>
    <select itemPath="result.date" produces="JSON">
      <urls>
        <url>http://www.gpw.pl/notowania_archiwalne</url>
      </urls>
      <execute><![CDATA[
        var results = request.get().response.match(/'\d{4}-\d{2}-\d{2}':1/g).map(function (match) {
            return match.substr(1, 10);
        });

        response.object = { date: results };
      ]]></execute>
    </select>
  </bindings>
</table>
