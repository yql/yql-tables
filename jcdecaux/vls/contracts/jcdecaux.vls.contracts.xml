<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
	<meta>
		<sampleQuery>SELECT * FROM {table} WHERE apiKey=</sampleQuery>
		<description>Get all contracts</description>
	</meta>
	<bindings>
		<select produces="JSON">
			<urls>
				<url>https://api.jcdecaux.com/vls/v1/contracts?{apiKey}</url>
			</urls>
			<inputs>
                          	<key id="apiKey" type="xs:string" paramType="query" required="true" />
			</inputs>
		</select>
	</bindings>
</table>