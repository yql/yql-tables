<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Jamie Matthews</author>
    <description>YQL table for Last.fm Tag.getTopTags API method. Fetches the top global tags on Last.fm, sorted by popularity (number of times used)</description>
    <documentationURL>http://www.last.fm/api/show?service=276</documentationURL>
  </meta>
  <bindings>
    <select itemPath="" produces="XML">
      <urls>
        <url>http://ws.audioscrobbler.com/2.0/?method=tag.gettoptags</url>
      </urls>
      <inputs>
        <key id="api_key" type="xs:string" paramType="query" required="true" />
        <key id="tag" type="xs:string" paramType="query" required="true" />
      </inputs>
    </select>
  </bindings>
</table>
