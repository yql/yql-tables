            </inputs>
            <execute>
                <![CDATA[
              		
              		var myvalue = \'hello \'   bob;
                    
              		response.object = <hi>{myvalue}</hi>;
              
              
                ]]>
            </execute>
        </select>
     </bindings>
</table>