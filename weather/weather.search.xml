<?xml version="1.0"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Sam Pullara</author>
    <documentationURL>http://xoap.weather.com</documentationURL>
  </meta>
  <bindings>
    <select produces="XML" itemPath="search.loc">
      <urls>
        <url>http://xoap.weather.com/search/search?where={query}</url>
      </urls>
      <inputs>
        <key id="query" type="xs:string" paramType="path" required="true"/>
      </inputs>
    </select>
  </bindings>
</table>