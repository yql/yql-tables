<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Mike Dillon</author>
    <sampleQuery>select * from {table}</sampleQuery>
    <!--<documentationURL></documentationURL>-->
  </meta>
  <bindings>
    <select itemPath="api.query.namespaces.ns" produces="XML">
      <urls>
        <url>http://{site}/w/api.php?action=query&amp;format=xml&amp;meta=siteinfo&amp;siprop=namespaces</url>
      </urls>
      <!--
      <paging model="offset">
        <start default="0" id="dummy"/>
        <pagesize max="500" id="rclimit"/>
        <total default="10"/>
      </paging>
      </paging>
      -->
      <inputs>
        <key id="site" type="xs:string" paramType="path" required="true" default="en.wikipedia.org"/>
      </inputs>
    </select>
  </bindings>
</table>
